function Team(name, record, percentage, differential, homers, BestPlayer) {
    this.name = name;
    this.record = record;
    this.percentage = percentage;
    this.differential = differential;
    this.homers = homers;
    this.BestPlayer = BestPlayer;
    this.role = "";
}

Team.prototype.setRole = function(role) {
    this.role = role;
}

Team.prototype.toJSON = function() {
    const obj = {name: this.name, record: this.record, percentage: this.percentage, differential: this.differential, homers: this.homers, BestPlayer: this.BestPlayer, role: this.role};
    const json = JSON.stringify(obj);
    return json;
}

var al = new Team("Houston Astros", "106-56", ".654", "+219", "214", "Yordan Alvarez, Justin Verlander");
LogItType(al);
